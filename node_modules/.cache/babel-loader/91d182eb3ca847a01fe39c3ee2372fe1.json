;\r\n    },\r\n    \r\n    previous() {\r\n      this.$refs.flickity.previous();\r\n    },\r\n    getPosts(){\r\n      axios.get('http://localhost:5000/posts')\r\n      .then(response => (this.posts = response.data))\r\n      .catch(error => {\r\n            this.errors.push(error);\r\n          })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>\r\n"],"sourceRoot":""}]}